{
  "name": "flame-match-monorepo",
  "version": "1.0.0",
  "private": true,
  "description": "Modern dating platform - Frontend + Backend",
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "npm run dev --prefix ./",
    "dev:backend": "npm run dev --prefix ./server",
    "build": "npm run build --prefix ./server && npm run build --prefix ./",
    "build:backend": "npm run build --prefix ./server",
    "build:frontend": "npm run build --prefix ./",
    "start:backend": "npm start --prefix ./server",
    "type-check": "npm run type-check --prefix ./server",
    "preview": "npm run preview --prefix ./"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
